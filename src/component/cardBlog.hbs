{{#with data}}
<div class="card">
                <img src="{{img}}" alt="image" class="card_image">
                <h2 class="card_title">{{title}}</h2>
                <p class="card_text">{{text}}</p>
                <a href="{{href}}" class="button card_button">{{button}}</a>
            </div>
            {{/with}}