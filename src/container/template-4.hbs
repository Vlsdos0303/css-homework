        <header class="header">
            <div class="">
            {{>logo}}
            </div>
            <nav class="header_nav">
                <div class="header_nav_right">
                {{>nav list=header }}
                </div>
            </nav>
        </header>

{{#with main}}
        <div class="main">
            <div class="main_content">
                <h1 class="main_title">{{heading}}</h1>
                <p class="main_text">{{descriotion}}</p>
                <a href="{{href}}" class="main_button">{{button}}</a>
            </div>
            <img src="{{image}}" alt="Main image" class="main_image">
        </div>
 {{/with}}

        <main class="featured">
            {{#each featured}}
            {{> cardFeatured data=this}}
            {{/each}}
        </main>


<div class="actions">
      {{#each actions}}
        {{> actionsContent data=this}}
         {{/each}}
         </div>

  
       <footer class="footer">
        <div class="col-3">
            {{>logo}}
            <span class="footer_copyright">Copyright 2023-2024</span>
        </div>
        {{#each footer}}
        <div>
           <span class="footer_title">{{title}}</span> 
           {{>nav list=links isVertical=true}}
        </div>
        {{/each}}
     </footer>